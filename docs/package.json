{
	"name": "pragmate-ui-docs",
	"version": "0.0.37",
	"title": "Pragmate UI",
	"description": "Official pragmate ui documentation with description of the components and uses of these",
	"license": "ISC",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"libraries": {
		"imports": [
			"pragmate-ui"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6500
				}
			},
			{
				"name": "web.tsc",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6501
				},
				"ts": {
					"compiler": "tsc"
				}
			}
		]
	},
	"static": {
		"includes": [
			"assets"
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.8",
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/react-18-widgets": "~0.0.5",
		"@beyond-js/reactive": "^1.1.3",
		"@emotion/css": "^11.11.2",
		"framer-motion": "^10.16.4",
		"framer-motion-3d": "^10.16.4",
		"is-mobile": "^4.0.0",
		"perfect-scrollbar": "^1.5.5",
		"prism-react-renderer": "^1.3.5",
		"prismjs": "^1.27.0",
		"react-select": "^5.7.3",
		"scheduler": "^0.23.0",
		"socket.io-client": "^4.5.4",
		"swiper": "^10.3.0",
		"tinycolor2": "^1.6.0",
		"tippy.js": "^6.3.7"
	},
	"devDependencies": {
		"@mdx-js/mdx": "^2.3.0",
		"@mdx-js/react": "^2.3.0",
		"@types/node": "^20.14.6",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"mdx-bundler": "^9.2.1"
	}
}
